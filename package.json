{
  "name": "prompt-generator",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run db:generate:skip-validation && tsc && vite build",
    "preview": "vite preview",
    "start": "node server/index.js",
    "start:with-migrate": "npm run db:migrate:deploy && node server/index.js",
    "server": "node server/index.js",
    "server:dev": "nodemon server/index.js",
    "sync:models": "tsx scripts/sync-models.ts",
    "db:generate": "prisma generate",
    "db:generate:skip-validation": "DATABASE_URL=\"postgresql://dummy:dummy@localhost:5432/dummy\" prisma generate",
    "postinstall": "export DATABASE_URL=\"postgresql://dummy:dummy@localhost:5432/dummy\" && npm run db:generate:skip-validation || echo 'Prisma generate skipped'",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:library": "tsx scripts/seed-prompt-library.ts",
    "db:generate:ai-templates": "tsx scripts/generate-ai-templates.ts",
    "db:update-admin": "node scripts/update-admin.js",
    "build:server": "tsc -p tsconfig.server.json",
    "dev:server": "tsc -p tsconfig.server.json --watch",
    "check:env": "node scripts/check-env.js",
    "pre-deploy": "node scripts/pre-deploy-check.js",
    "seo:generate": "node scripts/generate-seo-meta.js",
    "ai-services:parse": "node scripts/parse-ai-services.js",
    "ai-services:update": "node -e \"require('./server/scheduler/aiServiceScheduler').runAIServiceUpdate('manual')\"",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "@prisma/client": "^6.19.0",
    "@sentry/node": "^10.32.1",
    "@sentry/react": "^10.32.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.5.1",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "pino-roll": "^4.0.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "validator": "^13.15.23"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/validator": "^13.15.10",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "prisma": "^6.19.0",
    "sharp": "^0.34.5",
    "tsx": "^4.19.2",
    "typescript": "^5.2.2",
    "vite": "^7.2.4"
  }
}

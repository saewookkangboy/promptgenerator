    "@vitejs/plugin-react": "^4.2.1",
    "nodemon": "^3.1.11",
    "prisma": "^5.19.1",
    "sharp": "^0.34.5",
    "tsx": "^4.19.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
